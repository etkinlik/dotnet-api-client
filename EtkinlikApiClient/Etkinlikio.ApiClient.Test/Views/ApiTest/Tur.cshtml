@model IEnumerable<EtkinlikIO.ApiClient.Models.Tur>

@{
    ViewBag.Title = "Tur";
}

<h2>Tur</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.adi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.radi)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.adi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.radi)
        </td>
        <td>
            <a href="~/ApiTest/TurEtkinlik/@item.id">Türe Ait Etkinlik Listesi -></a>
        </td>
    </tr>
}

</table>
